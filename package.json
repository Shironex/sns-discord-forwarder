{
  "name": "sns-discord-forwarder",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "build": "tsup && copyfiles -u 1 \"src/public/**/*\" dist",
    "lint": "eslint src/**/*.{ts,js} --fix",
    "format:check": "prettier --check 'src/**/*.{ts,js,json,md}'",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "chalk": "^4.1.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3"
  }
}
